{
    "ontology": "imageedit.ontology.json",
    "agendas": {
        "train": "./sampled_100/agenda.train.pickle",
        "test": "./sampled_100/agenda.test.pickle"
    },
    "world": {
        "verbose": false
    },
    "agents": {
        "user": {
            "user": "AgendaBasedUserSimulator",
            "patience": 30,
            "dice_threshold": 0.5,
            "gesture_threshold": 0.0
        },
        "channel": {
            "ontology": "imageedit.ontology.json",
            "channel": "MultimodalChannel",
            "speech_conf_mean": 1.0,
            "speech_conf_std": 0.0
        },
        "system": {
            "ontology": "imageedit.ontology.json",
            "visionengine": {
                "visionengine": "VisionEngineDatabase",
                "database_path": "./sampled_100/visionengine.annotation.pickle"
            },
            "policy": "DQNPolicy"
        },
        "photoshop": {
            "photoshop": "SimplePhotoshop",
            "client": false,
            "uri": "http://localhost:2005",
            "verbose": true
        }
    },
    "policy": {
        "num_epochs": 1,
        "batch_size": 32,
        "freeze_interval": 500,
        "burn_in": 1000,
        "load_replaymemory": null,
        "qnetwork": {
            "input_size": null,
            "hidden_size": 100,
            "output_size": null,
            "dropout_rate": 0.0,
            "optimizer": "AdamOptimizer",
            "learning_rate": 1e-3
        },
        "replaymemory": {
            "memory_size": 10000
        },
        "scheduler": {
            "scheduler": "LinearScheduler",
            "init_epsilon": 0.5,
            "min_epsilon": 0.05,
            "schedule_timesteps": 1e7
        }
    }
}